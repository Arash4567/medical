# An example controller showing a sample home page
GET     /                           controllers.HomeController.index
GET     /admin                      controllers.HomeController.adminLogin
GET     /addPerson                  controllers.HomeController.addPerson
GET     /add-person                 @controllers.HomeController.addPerson
POST    /add-patient                @controllers.HomeController.createUser

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
->      /webjars                                     webjars.Routes